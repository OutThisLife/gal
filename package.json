{
  "name": "gal",
  "version": "0.0.49",
  "description": "Git add, commit, and push in one line.",
  "main": "./dist/index.js",
  "bin": {
    "gal": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "git",
    "quick",
    "commit",
    "credentials",
    "libsecret",
    "GitHub"
  ],
  "author": "Talasan Nicholson <talasan.nicholson@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/OutThisLife/gal",
  "bugs": "https://github.com/OutThisLife/gal/issues",
  "scripts": {
    "lint": "eslint --fix src",
    "format": "prettier -w src",
    "build": "tsc",
    "dev": "NODE_ENV=development tsc -w",
    "prepare": "NODE_ENV=production yarn build",
    "gal": "NODE_ENV=development node dist/index.js"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "simple-git": "^3.7.1"
  }
